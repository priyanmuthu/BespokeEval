{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": false,
    "no-trailing-whitespace": false,
    "object-literal-sort-keys":false,
    "trailing-comma":false,
    "variable-name":false,
    "whitespace":false,
    "one-line":false
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}